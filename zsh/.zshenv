# envvars
export EDITOR="nvim"
#eval  "$(dircolors)"

# allows to go back in history with popd and see past dirs
alias cd="pushd"

# ls
alias ls="ls --color"
alias ll="ls -l"
alias la="ls -a"
alias lla="ls -la"

# Git stuff
alias gst="git status"
alias gpull="git pull --rebase"
alias gssave="git stash save"
alias gspop="git stash pop"

alias gn="git-number"
